/dts-v1/;
  
/ {
    description = "nanopi-m4 boot uImage";
    #address-cells = <1>;
  
    images {
		kernel {
            description = "kernel 4.4.y";
            data = /incbin/("./kernel.gz");
            type = "kernel";
            arch = "arm64";
            os = "linux";
			compression = "gzip";
			load = <0x02080000>;
			entry = <0x02080000>;
		};

        fdt_m4 {
            description = "nanopi-m4";
            data = /incbin/("./rk3399-nanopi-m4.dtb");
            type = "flat_dt";
            arch = "arm64";
			compression = "none";
			load = <0x01f00000>;
        };

        initramfs {
            description = "busybox";
            data = /incbin/("./ramdisk.cpio.gz");
            type = "ramdisk";
            arch = "arm64";
            os = "linux";
			compression = "none";
			load = <0x06000000>;
			entry = <0x06000000>;
        };
    };

    configurations {
        default = "conf_m4";
        conf_m4 {
            description = "nanopi-m4";
            kernel = "kernel";
            ramdisk = "initramfs";
            fdt = "fdt_m4";
        };
    };
};
